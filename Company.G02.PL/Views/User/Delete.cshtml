@model UserToReturnDto
@{
    ViewData["Title"] = "Delete";
}

<h1>Delete User</h1>
<br />
<br />

<form asp-action="Delete">

    <div class="my-3">
        <label asp-for="UserName" class="col-form-label"></label>
        <input asp-for="UserName" class="form-control" readonly />
    </div>
    <div class="my-3">
        <label asp-for="FirstName" class="col-form-label"></label>
        <input asp-for="FirstName" class="form-control" readonly />
    </div>
    <div class="my-3">
        <label asp-for="LastName" class="col-form-label"></label>
        <input asp-for="LastName" class="form-control" readonly />
    </div>
    <div class="my-3">
        <label asp-for="Email" class="col-form-label"></label>
        <input asp-for="Email" class="form-control" readonly />
    </div>
    <div class="my-3">
        <input type="submit" value="Delete" class="btn btn-danger" />
        <a asp-action="Index" class="btn btn-light">Back To List</a>
    </div>
</form>

